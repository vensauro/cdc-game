    "nanoid": "^5.0.2",
    "next": "^13.4.8",
    "next-auth": "^4.22.1",
    "radash": "^11.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-spinners": "^0.13.8",
    "superjson": "^1.12.4",
    "tailwind-merge": "^1.14.0",
    "tsx": "^3.12.7",
    "ws": "^8.0.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.26.1",
    "@tanstack/react-query-devtools": "^4.18.0",
    "@types/node": "^18.16.16",
    "@types/react": "^18.2.8",
